{
  "name": "preact-f7-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "test": "eslint src && preact test"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-config-synacor": "^2.0.0",
    "if-env": "^1.0.0",
    "preact-cli": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "feathers-client": "^2.4.0",
    "feathers-offline-realtime": "^0.1.2",
    "feathers-redux": "^1.1.0",
    "feathers-rest": "^1.8.0",
    "framework7": "^2.0.0-beta.8",
    "lodash": "^4.17.4",
    "logdown": "^3.1.0",
    "node-polyglot": "^2.2.2",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.3",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.0.3"
  }
}
